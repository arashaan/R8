<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<PackageId>R8.GoogleAnalytics</PackageId>
		<Version>1.10.0</Version>
		<Authors>Arash Shabbeh</Authors>
		<Company>iamr8</Company>
		<PackageDescription>Handle files easily</PackageDescription>
		<RepositoryUrl>https://github.com/iamr8/R8</RepositoryUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>DEBUG;TRACE</DefineConstants>
		<Optimize>false</Optimize>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Google.Apis.AnalyticsReporting.v4" Version="1.49.0.2128" />
		<PackageReference Include="Google.Apis.Auth.AspNetCore3" Version="1.49.0" />
	</ItemGroup>
</Project>